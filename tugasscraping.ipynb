   "source": [
    "import requests\n",
    "from bs4 import BeautifulSoup"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {},
   "outputs": [],
   "source": [
    "root = 'https://myanimelist.net/topanime.php?limit=0'\n",
    "\n",
    "quotes = []\n",
    "authors = []\n",
    "links = []\n",
    "\n",
    "for page in range(1, 3):\n",
    "  url = requests.get(root + str(page))\n",
    "  soup = BeautifulSoup(url.text, 'html.parser')\n",
    "\n",
    "  for q in soup.find_all(\"div\", {\"class\":\"detail\"}):\n",
    "    quotes.append(q.find(\"h3\", {\"class\":\"fl-l fs14 fw-b anime_ranking_h3\"}).text)\n",
    "    \n",
    "    link_tag = q.find(\"h3\", {\"class\": \"fl-l fs14 fw-b anime_ranking_h3\"}).find('a')\n",
    "    if link_tag and 'href' in link_tag.attrs:\n",
    "      links.append(link_tag['href'])\n",
    "\n",
    "  for a in soup.find_all(\"div\", {\"class\":\"detail\"}):\n",
    "    authors.append(a.find(\"div\", {\"class\":\"information di-ib mt4\"}).text)\n",
    "\n",
   ]
  },
